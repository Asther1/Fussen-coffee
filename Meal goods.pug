mixin Meal-box(img,text)
     div(class="Meal-box")
        a(href="")
            img(src="imgs/"+img+".png", alt="產品 照片")
            p=text
            
<!DOCTYPE html>
html(lang="en")
    head
        meta(charset="zh")
        title Fussen Coffee
        link(rel="stylesheet", href="style.css")
    body
        div(id="fussen")
            nav
                img(src="imgs/福森.png", alt="Fussen Coffee首頁" class="logo")
                div(class="link")
                    a(href="index.html") 首頁
                    a(href="Meal goods.html") 餐點商品
                    a(href="Branch information.html") 分店資訊
                    a(href="contact us.html") 聯繫我們        
        section(id="Meal")
            +Meal-box("Asther1","套餐")
            +Meal-box("Asther2","沙拉")
            +Meal-box("Asther3","三明治")
            +Meal-box("Asther4","點心")
            +Meal-box("Asther5","咖啡")
            +Meal-box("Asther6","飲品")
        footer
            nav
                img(src="imgs/福森.png", alt="Fussen Coffee首頁" class="logo")
                div(class="link")
                    a(href="index.html") 首頁
                    a(href="Meal goods.html") 餐點商品
                    a(href="Branch information.html") 分店資訊
